<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0"
    xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    xmlns:granite="http://www.adobe.com/jcr/granite/1.0"
    jcr:primaryType="nt:unstructured">
    <patterns jcr:primaryType="nt:unstructured">
        <complexmultifield/>
    </patterns>
    <replacement jcr:primaryType="nt:unstructured">
        <complexmultifield
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
            granite:class="cui-complexmultifield multifield-full-width with-separator"
            composite="{Boolean}true"
            fieldLabel="${./fieldLabel}"
            cq:rewriteFinal="{Boolean}true">
            <field
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/container"
                name="${./name}">
                <items
                    cq:alterNames="{Boolean}true"
                    cq:rewriteMapChildren="./fieldConfigs"
                    jcr:primaryType="nt:unstructured"
                    cq:rewriteFinal="{Boolean}true"/>
            </field>
        </complexmultifield>
    </replacement>
</jcr:root>
